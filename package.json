{
  "name": "registry-sync",
  "version": "3.2.5",
  "description": "synchronize a remote npm registry for private use",
  "repository": "https://github.com/heikkipora/registry-sync",
  "bin": {
    "registry-sync": "bin/sync"
  },
  "scripts": {
    "build": "./build-npm",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check --loglevel warn .",
    "eslint": "eslint --fix --format=codeframe --ext .ts .",
    "eslint:check": "eslint --max-warnings=0 --format=codeframe --ext .ts .",
    "lint-staged": "lint-staged --verbose",
    "test": "mocha -r ts-node/register --timeout 120000 test/*.ts",
    "release-test": "cd release-test && ./run-sync-install-cycle.sh"
  },
  "author": "Heikki Pora",
  "license": "MIT",
  "dependencies": {
    "@yarnpkg/lockfile": "1.1.0",
    "axios": "0.21.1",
    "commander": "8.1.0",
    "semver": "7.3.5",
    "ssri": "8.0.1",
    "tar-fs": "2.1.1"
  },
  "devDependencies": {
    "@arkweid/lefthook": "0.7.6",
    "@types/chai": "4.2.21",
    "@types/lodash": "4.14.171",
    "@types/mocha": "9.0.0",
    "@types/node": "16.4.9",
    "@types/semver": "7.3.8",
    "@types/ssri": "7.1.1",
    "@types/tar-fs": "2.0.1",
    "@types/yarnpkg__lockfile": "1.1.5",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "chai": "4.3.4",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-mocha": "9.0.0",
    "express": "4.17.1",
    "lint-staged": "11.1.1",
    "mocha": "9.0.3",
    "prettier": "2.3.2",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  },
  "keywords": [
    "registry",
    "synchronize",
    "mirror",
    "private",
    "offline"
  ],
  "engines": {
    "node": ">=12.0.0"
  }
}
